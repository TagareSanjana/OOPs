class GrandFather():
    def __init__(self):
        self._a=10 
        self.__b = 20 
    def _fun1(self):
        print("This is our Farm")
    def __fun2(self):
        print("This is my Farmhouse") 
class Father(GrandFather):
    def fun3(self):
        print("I have 50cr") 
ob = Father() 
ob._GrandFather__fun2() 
